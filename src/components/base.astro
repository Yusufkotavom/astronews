---
// Astro Template for Website Content
// Import JSON data directly
import data from '../data/jasa_pembuatan_website_bekasi_meta_with_images.json';

// TypeScript interfaces for type safety
interface Business {
  type: string;
  address: string;
  phone: string;
  description: string;
}

interface Hero {
  badge_text: string;
  title: string;
  subtitle: string;
  feature: Array<{
    point: string;
    description: string;
  }>;
  image_url: string;
}

interface Features {
  badge_text: string;
  title: string;
  subtitle: string;
  point: Array<{
    title: string;
    description: string;
  }>;
  image_url: string;
}

interface Info {
  img_alt: string;
  badge_text: string;
  title: string;
  subtitle: string;
  description: string;
  point: string[];
  image_url: string;
}

interface Services {
  badge_text: string;
  title: string;
  subtitle: string;
  point: Array<{
    title: string;
    description: string;
    feature: string[];
  }>;
}

interface WhyUs {
  badge_text: string;
  title: string;
  subtitle: string;
  description: string;
  point: Array<{
    title: string;
    description: string;
  }>;
}

interface WhatYouGet {
  title: string;
  subtitle: string;
  points: Array<{
    title: string;
    description: string;
  }>;
}

interface WhyNeed {
  badge_text: string;
  title: string;
  subtitle: string;
  description: string;
  point: Array<{
    title: string;
    description: string;
  }>;
}

interface Pricing {
  badge_text: string;
  title: string;
  description: string;
  plan: Array<{
    badge: string;
    title: string;
    price: string;
    description: string;
    feature: string[];
  }>;
}

interface Portfolio {
  title: string;
  description: string;
  list: Array<{
    type: string;
    title: string;
    description: string;
  }>;
}


interface Testimonials {
  badge_text: string;
  title: string;
  subtitle: string;
  card: Array<{
    headline: string;
    quote: string;
    name: string;
    position: string;
  }>;
}

interface FAQ {
  badge_text: string;
  title: string;
  subtitle: string;
  points: Array<{
    question: string;
    answer: string;
  }>;
}

interface Content {
  title: string;
  subtitle: string;
  description: string;
  image_url: string;
}

interface Local {
  title: string;
  subtitle: string;
  description: string;
  city: Array<{
    title: string;
    description: string;
  }>;
}

interface KabupatenKota {
  nama: string;
  description: string;
  kecamatan: Array<{
    nama: string;
    link: string;
  }>;
}

interface FinalCTA {
  badge_text: string;
  title: string;
  subtitle: string;
}

interface SEOMeta {
  seo_data: {
    metadata: {
      meta_title: string;
      meta_description: string;
      canonical_url: string;
    };
    schema_markup: {
      json_ld_script: string;
    };
  };
}

interface GeneratedImages {
  generated_at: string;
  total_images: number;
  search_keyword: string;
  source: string;
}

// Extract data with type safety
const {
  business,
  hero,
  features,
  info,
  services,
  why_us,
  what_you_get,
  why_need,
  pricing,
  portfolio,
  testimonials,
  faq,
  content,
  local,    
  kabupaten_kota,
  final_cta,
  seo_meta,
  keyword,
  generated_images
}: {
  business: Business;
  hero: Hero;
  features: Features;
  info: Info;
  services: Services;
  why_us: WhyUs;
  what_you_get: WhatYouGet;
  why_need: WhyNeed;
  pricing: Pricing;
  portfolio: Portfolio;
  testimonials: Testimonials;
  faq: FAQ;
  content: Content;
  local: Local;
  kabupaten_kota: KabupatenKota[];
  final_cta: FinalCTA;
  seo_meta: SEOMeta;
  keyword: string;
  generated_images: GeneratedImages;
} = data;
---

<!DOCTYPE html>
<html lang="id">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <!-- SEO Meta Tags -->
    <title>{seo_meta.seo_data.metadata.meta_title}</title>
    <meta name="description" content={seo_meta.seo_data.metadata.meta_description} />
    <link rel="canonical" href={seo_meta.seo_data.metadata.canonical_url} />
    
    <!-- Schema Markup -->
    <script type="application/ld+json" set:html={seo_meta.seo_data.schema_markup.json_ld_script}></script>
    
    <!-- Open Graph Tags -->
    <meta property="og:title" content={business.type} />
    <meta property="og:description" content={business.description} />
    <meta property="og:phone" content={business.phone} />
    <meta property="og:address" content={business.address} />
    
    <!-- Tailwind CSS is handled by @astrojs/tailwind integration -->
    
    <!-- Custom Styles -->
    <style>
        .badge {
            @apply inline-block px-3 py-1 text-sm font-medium text-blue-600 bg-blue-100 rounded-full;
        }
        .btn-primary {
            @apply inline-flex items-center px-6 py-3 text-base font-medium text-white bg-blue-600 border border-transparent rounded-md shadow-sm hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500;
        }
        .btn-secondary {
            @apply inline-flex items-center px-6 py-3 text-base font-medium text-blue-600 bg-white border border-blue-600 rounded-md shadow-sm hover:bg-blue-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500;
        }
    </style>
</head>
